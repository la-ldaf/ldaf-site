{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "framework": "sveltekit",
  "installCommand": "npm install",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "content-security-policy",
          "value": "frame-ancestors 'self' https://app.contentful.com"
        }
      ]
    }
  ]
}
