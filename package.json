{
  "name": "ldaf-site",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prepare": "svelte-kit sync",
    "dev": "vite dev",
    "build": "node scripts/pre-build.js && vite build",
    "preview": "vite preview",
    "test": "npm run lint && npm run check && npm run test:unit",
    "test:e2e": "playwright test",
    "test:e2e:ui": "npm run test:e2e -- --ui",
    "test:unit": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --compiler-warnings \"css-unused-selector:ignore\"",
    "check:watch": "npm run check -- --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "chromatic",
    "bundle-visualizer": "node scripts/pre-build.js && mkdir -p ./bundle-visualizer && vite-bundle-visualizer --open=false -o bundle-visualizer/index.html"
  },
  "devDependencies": {
    "@playwright/test": "^1.33.0",
    "@poppanator/sveltekit-svg": "^3.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.1",
    "@rollup/plugin-typescript": "^11.1.0",
    "@storybook/addon-essentials": "^7.1.0-alpha.6",
    "@storybook/addon-interactions": "^7.1.0-alpha.6",
    "@storybook/addon-links": "^7.1.0-alpha.6",
    "@storybook/addon-viewport": "^7.1.0-alpha.6",
    "@storybook/blocks": "^7.1.0-alpha.6",
    "@storybook/svelte": "^7.1.0-alpha.6",
    "@storybook/sveltekit": "^7.1.0-alpha.6",
    "@storybook/testing-library": "^0.1.0",
    "@sveltejs/adapter-vercel": "^2.4.3",
    "@sveltejs/kit": "^1.15.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.2",
    "@testing-library/user-event": "^14.4.3",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.1",
    "@vitest/coverage-c8": "^0.30.1",
    "autoprefixer": "^10.4.7",
    "blurhash": "^2.0.5",
    "chromatic": "^6.17.3",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-svelte3": "^4.0.0",
    "glob": "^10.2.2",
    "jsdom": "^22.0.0",
    "postcss": "^8.4.23",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.21.4",
    "sass": "^1.62.1",
    "sharp": "^0.32.1",
    "storybook": "^7.1.0-alpha.6",
    "storybook-addon-designs": "^7.0.0-beta.2",
    "svelte": "^3.58.0",
    "svelte-check": "^3.2.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.3",
    "vite-bundle-visualizer": "^0.6.0",
    "vite-imagetools": "^5.0.2",
    "vite-plugin-svelte-purgecss": "^0.0.3",
    "vitest": "^0.30.1"
  },
  "type": "module",
  "dependencies": {
    "@uswds/uswds": "^3.4.1",
    "fast-blurhash": "^1.1.2"
  }
}
