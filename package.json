{
  "name": "ldaf-site",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prepare": "svelte-kit sync && npm run gen-operations",
    "predev": "npm run prebuild",
    "dev": "concurrently --kill-others --names graphql,dev \"npm run gen-operations -- --watch\" \"vite dev\"",
    "prebuild": "./scripts/prebuild.ts && npm run gen-operations",
    "build": "vite build",
    "preview": "vite preview",
    "pretest": "./scripts/pretest.ts && npm run gen-operations",
    "test": "concurrently --kill-others --raw \"npm run gen-operations -- --watch --silent\" \"npm run lint && npm run check && npm run test:unit\"",
    "test:e2e": "playwright test",
    "test:e2e:ui": "npm run test:e2e -- --ui",
    "test:unit": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "precheck": "./scripts/precheck.ts && npm run gen-operations",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --compiler-warnings \"css-unused-selector:ignore\"",
    "check:watch": "npm run check -- --watch",
    "lint": "prettier --plugin=prettier-plugin-svelte --check . && eslint .",
    "format": "prettier --plugin=prettier-plugin-svelte --write .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "npm run prebuild && storybook build",
    "bundle-visualizer": "npm run prebuild && mkdir -p ./bundle-visualizer && vite-bundle-visualizer --open=false -o bundle-visualizer/index.html",
    "gen-schema": "graphql-codegen --config codegen-schema.yml --require dotenv/config",
    "gen-operations": "graphql-codegen --config codegen-operations.yml"
  },
  "devDependencies": {
    "@contentful/rich-text-from-markdown": "^15.16.6",
    "@contentful/rich-text-html-renderer": "^16.1.0",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/near-operation-file-preset": "^2.5.0",
    "@graphql-codegen/schema-ast": "^4.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-eslint/eslint-plugin": "^3.20.1",
    "@playwright/test": "^1.36.1",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.2",
    "@storybook/addon-essentials": "^7.3.1",
    "@storybook/addon-interactions": "^7.3.1",
    "@storybook/addon-links": "^7.3.1",
    "@storybook/addon-viewport": "^7.3.1",
    "@storybook/blocks": "^7.3.1",
    "@storybook/svelte": "^7.3.1",
    "@storybook/sveltekit": "^7.3.1",
    "@storybook/testing-library": "^0.2.0",
    "@storybook/theming": "^7.3.1",
    "@sveltejs/adapter-vercel": "^3.0.2",
    "@sveltejs/kit": "^1.22.6",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/svelte": "^4.0.3",
    "@testing-library/user-event": "^14.4.3",
    "@types/prettier": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@vitest/coverage-v8": "^0.34.1",
    "autoprefixer": "^10.4.7",
    "blurhash": "^2.0.5",
    "concurrently": "^8.2.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-svelte": "^2.32.4",
    "glob": "^10.3.3",
    "graphql": "^16.7.1",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.27",
    "postcss-load-config": "^4.0.1",
    "prettier": "^3.0.2",
    "prettier-plugin-svelte": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.26.3",
    "sass": "^1.64.1",
    "sharp": "^0.32.4",
    "storybook": "^7.3.1",
    "storybook-addon-designs": "^7.0.0-beta.2",
    "svelte": "^4.0.0",
    "svelte-check": "^3.4.6",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vite-bundle-visualizer": "^0.8.1",
    "vite-imagetools": "^5.0.5",
    "vite-plugin-svelte-purgecss": "^0.0.3",
    "vitest": "^0.34.1"
  },
  "type": "module",
  "dependencies": {
    "@contentful/rich-text-types": "^16.0.3",
    "@poppanator/sveltekit-svg": "^4.1.0",
    "@uswds/uswds": "^3.4.1",
    "algoliasearch": "^4.19.1",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "devalue": "^4.3.2",
    "fast-blurhash": "^1.1.2",
    "graphql": "^16.7.1",
    "graphql-tag": "^2.12.6",
    "instantsearch.js": "^4.56.9",
    "jpeg-js": "^0.4.4",
    "lodash": "^4.17.21",
    "redis": "^4.6.7",
    "web-vitals": "^3.4.0"
  }
}
